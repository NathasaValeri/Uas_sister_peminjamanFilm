{
  "version": 2,
  "builds": [
    { "src": "auth-service.js", "use": "@vercel/node" },
    { "src": "movie-service.js", "use": "@vercel/node" },
    { "src": "rental-service.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/auth/(.*)", "dest": "auth-service.js" },
    { "src": "/movies/(.*)", "dest": "movie-service.js" },
    { "src": "/rentals/(.*)", "dest": "rental-service.js" }
  ]
}
